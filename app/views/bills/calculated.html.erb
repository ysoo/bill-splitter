<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Person</th>
        <th scope="col">Amount</th>
      </tr>
    </thead>
    <tbody>
      <% @values.each do |key, value| %>
        <tr>
          <th scope="row"><%= key %></th>
          <td> <%= value %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
